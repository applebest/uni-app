#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/clt/Desktop/uni-vue3-vite-ts-pinia-main/node_modules/.pnpm/@commitlint+cli@17.8.1/node_modules/@commitlint/cli/node_modules:/Users/clt/Desktop/uni-vue3-vite-ts-pinia-main/node_modules/.pnpm/@commitlint+cli@17.8.1/node_modules/@commitlint/node_modules:/Users/clt/Desktop/uni-vue3-vite-ts-pinia-main/node_modules/.pnpm/@commitlint+cli@17.8.1/node_modules:/Users/clt/Desktop/uni-vue3-vite-ts-pinia-main/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/clt/Desktop/uni-vue3-vite-ts-pinia-main/node_modules/.pnpm/@commitlint+cli@17.8.1/node_modules/@commitlint/cli/node_modules:/Users/clt/Desktop/uni-vue3-vite-ts-pinia-main/node_modules/.pnpm/@commitlint+cli@17.8.1/node_modules/@commitlint/node_modules:/Users/clt/Desktop/uni-vue3-vite-ts-pinia-main/node_modules/.pnpm/@commitlint+cli@17.8.1/node_modules:/Users/clt/Desktop/uni-vue3-vite-ts-pinia-main/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
